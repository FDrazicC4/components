        "react-flip-toolkit": "^7.0.7",
        "react-hot-loader": "^4.12.18",
        "react-router-dom": "^5.1.2",
        "sass-loader": "^8.0.0",
        "typescript": "^3.7.3",
        "typescript-plugin-css-modules": "^2.1.1"
    },
    "peerDependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router-dom": "^5.1.2"
