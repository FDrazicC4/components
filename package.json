        "@storybook/react": "^5.2.1",
        "@storybook/theming": "^5.2.1",
        "@types/enzyme": "^3.10.3",
        "@types/jest": "^24.0.18",
        "@types/node": "^12.7.7",
        "@types/react": "^16.9.3",
