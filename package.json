{
    "name": "@brewkit/components",
    "version": "1.0.0",
    "description": "Brewkit components",
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Snap-AV/brewkit.git"
    },
    "author": "lawren",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Snap-AV/brewkit/issues"
    },
    "homepage": "https://github.com/Snap-AV/brewkit#readme",
    "scripts": {
        "test:generate-output": "jest --watchAll --json --outputFile=.build/jest/test-results.json || true",
        "test": "jest --watchAll --json --outputFile=.build/jest/test-results.json",
        "sass": "./node_modules/.bin/dart-sass --style=compressed ./theme/cerveza/src/index.scss ./.build/storybook/assets/css/theme.css",
        "sass-watch": "npm run sass -- --watch",
        "commit": "npx git-cz",
        "start": "npm run sass && npm run storybook",
        "storybook": "start-storybook -p 9001 -c .build/storybook",
        "build": "tsc --project ./.build/tsconfig.json --declaration"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/lib/"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|md)$": "<rootDir>/.build/jest/mocks/fileMock.js",
            "^.+\\.(css|less|scss)$": "identity-obj-proxy"
        },
        "setupFilesAfterEnv": [
            "<rootDir>/.build/jest/setupTests.js"
        ]
    },
    "devDependencies": {
        "@storybook/addon-actions": "^5.3.13",
        "@storybook/addon-backgrounds": "^5.3.13",
        "@storybook/addon-console": "^1.2.1",
        "@storybook/addon-docs": "^5.3.13",
        "@storybook/addon-jest": "^5.3.13",
        "@storybook/addon-knobs": "^5.3.13",
        "@storybook/addon-notes": "^5.3.13",
        "@storybook/addon-storyshots": "^5.3.13",
        "@storybook/addon-viewport": "^5.3.13",
        "@storybook/preset-typescript": "^1.2.0",
        "@storybook/react": "^5.3.13",
        "@storybook/theming": "^5.3.13",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^24.9.1",
        "@types/node": "^12.12.27",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@types/react-router-dom": "^5.1.3",
        "@types/react-test-renderer": "^16.9.2",
        "@typescript-eslint/eslint-plugin": "^2.19.2",
        "@typescript-eslint/parser": "^2.19.2",
        "awesome-typescript-loader": "^5.2.1",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-require-context-hook": "^1.0.0",
        "cache-loader": "^4.1.0",
        "cz-conventional-changelog": "^3.1.0",
        "dart-sass": "^1.25.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "eslint": "^6.7.2",
        "eslint-plugin-react": "^7.18.3",
        "fork-ts-checker-webpack-plugin": "^4.0.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.9.0",
        "prettier": "^1.19.1",
        "react": "^16.12.0",
        "react-docgen-typescript-loader": "^3.6.0",
        "react-dom": "^16.12.0",
        "react-flip-toolkit": "^7.0.7",
        "react-hook-form": "^4.9.3",
        "react-hot-loader": "^4.12.19",
        "react-router-dom": "^5.1.2",
        "react-test-renderer": "^16.12.0",
        "react-transition-group": "^4.3.0",
        "sass-loader": "^8.0.2",
        "ts-jest": "^24.3.0",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.5",
        "typescript-plugin-css-modules": "^2.1.1"
    },
    "peerDependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-router-dom": "^5.1.2"
    },
    "dependencies": {
        "@popperjs/core": "^2.0.6",
        "clsx": "^1.1.0",
        "lodash": "^4.17.15"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "release": {
        "branches": [
            "master",
            "next"
        ]
    }
}
