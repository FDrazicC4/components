        "@babel/plugin-syntax-import-meta": "^7.2.0",
        "@babel/plugin-transform-object-assign": "^7.2.0",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^5.1.9",
        "@storybook/addon-backgrounds": "^5.1.9",
